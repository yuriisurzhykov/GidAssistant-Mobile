<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/gidassistant_white"/>
            <padding android:top="@dimen/small_padding"
                android:right="@dimen/default_padding"
                android:left="@dimen/default_padding"
                android:bottom="@dimen/small_padding"/>
        </shape>
    </item>
    <item>
        <bitmap
            android:src="@drawable/ic_location"
            android:tint="@color/gidassistant_gray"
            android:gravity="center"/>
    </item>
</layer-list>